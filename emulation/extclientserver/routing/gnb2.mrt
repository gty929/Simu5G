ifconfig:

# interface to the radio access network
name: cellular
    inet_addr: 10.0.10.100
    Mask: 255.255.255.0
    MTU: 1500
    POINTTOPOINT MULTICAST
    
name: pppIf
    inet_addr: 10.0.11.2
    Mask: 255.255.255.0
    MTU: 1500
    POINTTOPOINT MULTICAST
ifconfigend.

route:
#Destination     Gateway          Genmask          Flags  Metric  Iface
10.0.10.0         *                255.255.255.0    H      0       cellular
10.0.11.0         *                255.255.255.0    H      0       pppIf
10.0.12.0         *                255.255.255.0    H      0       pppIf
192.168.2.0      *                255.255.255.0    H      0       pppIf
192.168.3.0      *                255.255.255.0    H      0       cellular
172.18.0.0       *                255.255.0.0      H      0       cellular
0.0.0.0          *                0.0.0.0          G      0       pppIf

routeend.

